{
  "title": "IcyTail's Blog",
  "author": "IcyTail",
  "description": "记录生活的点点滴滴",
  "language": "zh-hans",
  "gitbook": "3.2.3",
  "root": "./doc",
  "structure": {
    "readme": "README.md"
  },
  "links": {
    "sidebar": {
      "Home": "https://google.com"
    }
